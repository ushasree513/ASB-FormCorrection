/* Common Classes */

.head-title {
    line-height: 0.88;
    letter-spacing: -0.24px;
}

.radius-0 {
    border-radius: 0 !important;
}

.content-main {
    width: 100%;
    max-width: 1080px !important;
    padding: 31px 20px 48px 20px;
}

.btn-round-custom {
    border-radius: 20.5px;
    box-shadow: 0 0 16px 2px rgba(1, 49, 96, 0.36);
    font-size: 16px;
    /* height: 45px;
    padding: 0 35px; */
    margin-top: 35px;
    height: 40px;
    padding: 0 36px;
}


/*Header Styles*/

.header {
    box-shadow: 0 0 8.4px 0.6px rgba(107, 107, 107, 0.27);
    overflow: hidden;
    z-index: 1;
}

.block1 p,
.block2 p,
.block3 p,
.block4 p {
    margin-top: 10px;
}

.block1,
.block2,
.block3 {
    padding: 10px 20px;
}

.block4 {
    padding: 15px 15px;
}

.block4 img {
    transform: translate(0%, 15%);
    height: 36px;
    width: 36px;
}


/* Sidebar and Menu Styles */

.sidebar {
    max-width: 286px !important;
}

.menu li {
    background: none !important;
    color: #ffffff !important;
    padding: 15px 15px;
    font-size: 16px;
}

.menu li.active {
    background: #ebfffd !important;
    font-weight: bold;
    color: #0067b1 !important;
}

.menu li img {
    max-width: 40px;
}

.card {
    max-width: 32.333333% !important;
    box-shadow: 0 0 6.5px 0.5px rgba(195, 195, 195, 0.55);
}

.card-title {
    font-size: 21px;
}

.card-body {
    border: 0;
}

.card-body svg {
    fill: #fff !important;
    margin-right: -8px;
}

.card-2 .card-body svg,
.card-3 .card-body svg {
    fill: #000 !important;
}

.card-3 .btn_custom {
    border-radius: 10px;
    border: solid 1px #d6d6d6;
    background: #ffffff;
}

.ellipsis {
    width: 20px;
    height: 20px;
}

.card-2 .card-body .dropdown_custom,
.card-3 .card-body .dropdown_custom {
    background-color: #fff;
}

.card-body .dropdown_custom {
    height: 40px;
    border-radius: 10px;
    background: #e3e3e3;
    margin: 7px 0 0px 0;
    font-size: 16px;
    padding-left: 16px;
    background-image: url(../assets/images/down-arrow.png);
    -webkit-appearance: none;
    -moz-appearance: none;
    background-repeat: no-repeat;
    background-position-x: 95%;
    background-position-y: 15px;
}

.custom_num {
    font-weight: 600 !important;
    font-size: 17px;
    letter-spacing: 1.5px;
}

.btn_custom {
    height: 39px !important;
    border-radius: 10px !important;
    font-family: "Roboto", sans-serif;
    font-weight: normal;
    font-size: 16px !important;
}

.btn_custom:first-child {
    margin-right: 6px;
}


